<main role="main" class="container">

  <div class="starter-template">
    <h1>Characters</h1>
    <ul class="list-unstyled">
      <% @characters.each do |char| %>
      <li class="lead"><%= char.name %>, the <%= char.alignment %> <%= char.race %></li>
      <% end %>
    </ul>
    
  </div>

</main><!-- /.container -->

